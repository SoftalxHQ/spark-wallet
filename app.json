{
  "expo": {
    "name": "spark-wallet",
    "slug": "spark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "sparkwallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spark-splash.png",
        "backgroundColor": "#2C1A0B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.softalxsolutions.spark"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/spark-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2C1A0B",
          "duration": 1000,
          "fade": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "86c286a8-a6d2-4532-a74f-920af9d7cb0b"
      }
    },
    "owner": "softalx-solutions"
  }
}
